<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
    <link rel="stylesheet" href="./css/list.css">
</head>
<body>
    

    <h1 class="page-title">Registered Users for HackOFiesta</h1>
    <a href="/">Logout</a>

<div class="wrap">
  <table class="conversion-rate-table">
    <thead class="table__head">
      <tr class="table__headers">
        <th class="header" scope="col">Name</th>

        <th class="header" scope="col">Email</th>
      </tr>
    </thead>

    <tbody class="table__content">
        <% users.forEach(element => { %>
            <tr class="table__row">
                <td class="row__cell"><%= element.name %></td>
        
                <td class="row__cell"><%= element.email %></td>
              </tr>
        <% }) %>
    </tbody>
  </table>
</div>
</body>
</html>